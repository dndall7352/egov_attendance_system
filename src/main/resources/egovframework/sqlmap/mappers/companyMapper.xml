<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="egovframework.atoz.mobile.mapper.CompanyMapper">
	<select id="companyInfo" parameterType="int" resultType="CompanyVo">
		select *
		from company_info
		where com_number = #{com_number}
	</select>
</mapper>